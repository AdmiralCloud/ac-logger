{
  "name": "ac-logger",
  "author": "Mark Poepping (https://www.mmpro.de)",
  "license": "MIT",
  "repository": "mmpro/ac-logger",
  "version": "1.0.7",
  "dependencies": {
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "ac-conventional-changelog-template": "git+https://github.com/mmpro/ac-conventional-changelog-template.git",
    "corp-semantic-release": "git+https://github.com/mmpro/corp-semantic-release.git",
    "cz-customizable": "git+https://github.com/mmpro/cz-customizable.git",
    "eslint": "6.x",
    "expect": "^24.8.0",
    "mocha": "^6.1.4",
    "mocha-jenkins-reporter": "^0.4.1",
    "mocha-junit-reporter": "^1.23.0"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": ".czConfig.js"
    }
  },
  "scripts": {
    "test": "mocha --reporter spec",
    "test-jenkins": "JUNIT_REPORT_PATH=./report.xml mocha --colors --reporter mocha-jenkins-reporter --reporter-options junit_report_name='ACLogger'"
  },
  "engines": {
    "node": ">=8.0.0"
  }
}
